{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "server/dist",
        "baseUrl": ".",
        "paths": {
            "*": ["server/node_modules/*", "server/src/types/*"]
        }
    },
    "include": ["./server/src/**/*"]
    }